{
  "name": "smarthr-patterns",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "run-s storybook",
    "storybook": "start-storybook -p 8008",
    "build-storybook": "build-storybook",
    "lint": "run-p lint:*",
    "lint:js": "eslint './**/*.ts{,x}'",
    "lint:css": "echo \"後でちゃんとして! stylelint './**/*.ts{,x}'\"",
    "format": "run-p format:*",
    "format:js": "run-s 'lint:js --fix'",
    "format:css": "echo \"後でちゃんとして! runs-s 'lint:css --fix\""
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "smarthr-normalize-css": "^1.1.0",
    "smarthr-ui": "^24.3.0",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@storybook/addon-actions": "^6.5.14",
    "@storybook/addon-essentials": "^6.5.14",
    "@storybook/addon-interactions": "^6.5.14",
    "@storybook/addon-links": "^6.5.14",
    "@storybook/builder-vite": "^0.2.5",
    "@storybook/react": "^6.5.14",
    "@storybook/testing-library": "^0.0.13",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^3.0.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.29.0",
    "eslint-config-smarthr": "^6.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.0",
    "prettier-config-smarthr": "^1.0.0",
    "stylelint-config-smarthr": "^1.1.0",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "vite-tsconfig-paths": "^4.0.2"
  }
}
